{
  "name": "GitHub comment navigator",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "Jump to previous/next GitHub comments with j/k",
  "homepage_url": "https://github.com/banga/github-extension",
  "icons": {
    "256": "icons/icon_256.png"
  },
  "permissions": [
    "https://github.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "src/main.js"
      ],
      "css": [
        "src/main.css"
      ]
    }
  ]
}